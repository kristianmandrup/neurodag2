<div class="wrapper">    
    <div class="contentHeadline">
    	<span class="collumTopText">
        <%= @title %>
    	</span>
    </div>
</div>
<div class="formWrapper">
<% form_for :user do |form| %>
	<%= render :partial => 'form', :locals => {:form => form} %>
	<%= hidden_field_tag "attribute", "password", :id => nil %> 
	<%= submit_tag "OK", :class => "submit" %>
<% end %>
</div>
